<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.group.member.MemberDAO">
	<select id="selectListMember" resultType="MemberVO">
		select * from test
	</select>
	<!-- security login -->
	<select id="getMember" parameterType="MemberVO" resultType="MemberVO">
		select id,pw
		from member
		where id=#{id}
	</select>
	
	<!-- 회원 my page   -->
	<select id="getSelect" parameterType="String" resultType="MemberVO">
		SELECT *
		FROM member
		WHERE id=#{id}
	</select>
	
</mapper>